doctype html
html(lang="en")
  head
    meta(charset="UTF-8")
    title YU XUAN LEE's Front_End Portfolio
    link(rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Concert+One&display=swap")
    link(rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/meyer-reset/2.0/reset.min.css")
    link(rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.4.0/css/font-awesome.min.css")
    link(rel="stylesheet" href="../css/time.css")
  body
    header
      a(href="index.html")
        img(src="../img/logo_s.svg", alt="")
    .menu
      input(type="checkbox" name="toggle" id="toggle")
      label(for="toggle") 
        .cross
          .line1
          .line2
      ul
        li
          a(href="index.html") Home
        li
          a(href="front_end.html") Front-end
        li
          a(href="others.html") Others
    section.cd-horizontal-timeline
      .timeline
        .events-wrapper
          .events
            ol
              //- li(v-for="t in times.slice(1,5)")
              //-   a(href="#0" v-bind:data-date="t.datadate" v-bind:class="t.iscss") {{t.date.substr(0,6)}}
              li
                a.selected(href="#0" data-date="8/12/2017") 8 DEC
              li
                a(href="#0" data-date="8/4/2018") 8 APR
              li
                a(href="#0" data-date="5/6/2018") 5 JUN
            //-   li
            //-     a(href="#0" data-date="15/06/2019") 15 JUN
            //-   li
            //-     a(href="#0" data-date="10/08/2019") 10 AUG
            //-   li
            //-     a(href="#0" data-date="19/11/2019") 19 Nov
              //- li
              //-   a(href="#0" data-date="10/12/2014") 10 Dec
              //- li
              //-   a(href="#0" data-date="19/01/2015") 29 Jan
              //- li
              //-   a(href="#0" data-date="03/03/2015") 3 Mar
            span.filling-line(aria-hidden="true")
          // .events
        // .events-wrapper
        ul.cd-timeline-navigation
          li
            a.prev.inactive(href="#0") Prev
          li
            a.next(href="#0") Next
        // .cd-timeline-navigation
      // .timeline
      .events-content#app2
        ol
          li(v-for="w in works" v-bind:data-date="w.datadate" v-bind:class="w.iscss")
            em {{w.date}}
            h2 {{w.name}}
            br
            img(src="../img/breakline.svg", alt="")
            p {{w.tool}}
              br
              |{{w.responsibility}}
              br
              |{{w.content}}
            .workimgs(v-bind:style="{ 'background-color': w.workimgs_color}")
                .m_index(v-bind:style="{ 'background-color': w.m_index_color}")
                img.img1(:src="w.img1", alt="")
                img.img2(:src="w.img2", alt="")
            .bottom(v-bind:style="{ 'background-color': w.bottom_color}")
              a.view-btn(:href="w.link" target="_blank") View
